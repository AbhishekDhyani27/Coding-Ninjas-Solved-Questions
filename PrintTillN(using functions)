Print numbers from 1 to n using functions

""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""

public class ABC {
public static void printTillN(int n) {
        if(n<0) {
            return;
        }
        for(int i=1; i<=n; i++) {
            System.out.print(" "+i);
        }
    }
      public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        printTillN(n);
         }
}
